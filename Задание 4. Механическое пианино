#include <iostream>
#include <string>
#include<cmath>
#include <sstream>

using namespace std;

enum note
{
    DO = 1,
    RE = 2,
    MI = 4,
    FA = 8, 
    SOL = 16,
    LA = 32,
    SI = 64
};

int main(){
    string chord;
    do{
        cout << "Enter chord: "; cin >> chord;
        {
            int accord = stoi(chord);
            if(accord == 0){
                return 0;
            } 
        }

        int notes = 0;
    
        for(size_t i = 0; i < chord.length(); i++){
            char digit = chord[i];
            if(chord.length() > 7){
                cout << "Error.\n";
                return 0;
            }
            int index = digit - '1';
            int mask = 1 << index;
            notes |= mask;
        }
            if(notes & note::DO) cout << "DO\n";
            if(notes & note::RE) cout << "RE\n";
            if(notes & note::MI) cout << "MI\n";
            if(notes & note::FA) cout << "FA\n";
            if(notes & note::SOL) cout << "SOL\n";
            if(notes & note::LA) cout << "LA\n";
            if(notes & note::SI) cout << "SI\n";
    }while(true);
}
